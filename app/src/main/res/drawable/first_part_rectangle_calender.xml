<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:id="@+id/first_part_first_drawable">
        <shape android:shape="rectangle">
            <solid android:color="@android:color/white"/>
            <size android:height="10dp" android:width="20dp"/>
        </shape>
    </item>
    <item android:id="@+id/second_part_first_drawable" android:left="20dp" >
        <shape android:shape="rectangle">
            <solid android:color="@color/fav"/>
            <size android:width="20dp" android:height="10dp"/>
            <corners android:topLeftRadius="10dp" android:bottomLeftRadius="10dp"/>
        </shape>
    </item>
</layer-list>